
<urls>
        <name>Get Monitored URLs</name>
        <script_path>|path_php_binary| -q |path_cacti|/plugins/mURLin/scripts/query_sites.php</script_path>
        <arg_prepend>|host_id|</arg_prepend>
        <arg_num_indexes>count</arg_num_indexes>
        <arg_index>index</arg_index>
        <arg_query>query</arg_query>
        <arg_get>get</arg_get>
        <output_delimeter>!!</output_delimeter>
        <fields>
                <url>
                        <name>Site</name>
                        <direction>input</direction>
                        <query_name>sites</query_name>
                </url>
                <text>
                        <name>Text to Match</name>
                        <direction>input</direction>
                        <query_name>text</query_name>
                </text>
                <values>
                        <name>values</name>
                        <direction>output</direction>
                        <query_name>values</query_name>
                </values>
                <http_code>
                        <name>http_code</name>
                        <direction>output</direction>
                        <query_name>http_code</query_name>
                </http_code>
                <downloadsize>
                        <name>downloadsize</name>
                        <direction>output</direction>
                        <query_name>downloadsize</query_name>
                </downloadsize>
        </fields>
</urls>